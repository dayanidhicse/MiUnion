<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="com.example.home.miunion_v3" >

<uses-permission android:name="android.permission.INTERNET"/>

<application
    android:allowBackup="true"
    android:icon="@drawable/iconmi"
    android:label="@string/app_name"

    android:theme="@style/AppTheme" >
    <activity
        android:name="com.example.home.miunion_v3.Splash"
        android:label="@string/app_name" >
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity
        android:label="@string/app_name"
        android:name="com.example.home.miunion_v3.MainActivity" >
    </activity>
    <activity
        android:label="@string/app_name"
        android:name="com.example.home.miunion_v3.Register" >
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.example.home.miunion_v3.MainActivity" />
    </activity>
    <activity
        android:label="@string/app_name"
        android:name="com.example.home.miunion_v3.UserProfile" >
    </activity>
    <activity
        android:label="@string/app_name"
        android:name="com.example.home.miunion_v3.Response" >

    </activity>
    <activity
        android:label="@string/app_name"
        android:name="com.example.home.miunion_v3.Hospital" >
    </activity>
    <activity
        android:label="@string/app_name"
        android:name="com.example.home.miunion_v3.Admin" >
    </activity>

    <activity
        android:label="History"
        android:name="com.example.home.miunion_v3.History" >
    </activity>
    <activity
        android:label="Remote"
        android:name="com.example.home.miunion_v3.Remote" >

    </activity>
    <activity
        android:label="Location"
        android:name="com.example.home.miunion_v3.Location" >
    </activity>

    <activity
        android:label="Age"
        android:name="com.example.home.miunion_v3.Age" >
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.example.home.miunion_v3.Admin" />
    </activity>
    <activity
        android:label="Epidemic"
        android:name="com.example.home.miunion_v3.Epidemic" >
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.example.home.miunion_v3.Admin" />
    </activity>
    <activity
        android:label="Welfare"
        android:name="com.example.home.miunion_v3.Welfare" >
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.example.home.miunion_v3.Admin" />
    </activity>
    <activity
        android:label="About"
        android:name="com.example.home.miunion_v3.About" >

    </activity>
</application>


</manifest>


    